{
  "name": "sparta-social",
  "version": "0.0.1",
  "description": "A decentralized exercise activity tracker.",
  "author": "Daniel Doubrovkine",
  "license": "MIT",
  "main": "index.ts",
  "private": true,
  "scripts": {
    "dev": "tsx watch --clear-screen=false src/index.ts | pino-pretty",
    "build": "tsup",
    "start": "node dist/index.js",
    "lexgen": "lex gen-server ./src/lexicon ./lexicons/*",
    "clean": "rimraf dist coverage"
  },
  "dependencies": {
    "@atproto/api": "^0.13.4",
    "@atproto/common": "^0.4.1",
    "@atproto/identity": "^0.4.0",
    "@atproto/lexicon": "^0.4.2",
    "@atproto/oauth-client-node": "^0.2.0",
    "@atproto/sync": "^0.1.4",
    "@atproto/syntax": "^0.3.0",
    "@atproto/xrpc-server": "^0.7.0",
    "@mapbox/polyline": "^1.2.1",
    "better-sqlite3": "^11.1.2",
    "dotenv": "^16.4.5",
    "duration-pretty": "^0.1.1",
    "envalid": "^8.0.0",
    "express": "^4.21.2",
    "fit-file-parser": "^1.20.0",
    "iron-session": "^8.0.2",
    "kysely": "^0.27.4",
    "multer": "^1.4.5-lts.1",
    "multiformats": "^13.0.0",
    "pino": "^9.3.2",
    "uhtml": "^4.5.9"
  },
  "devDependencies": {
    "@atproto/lex-cli": "^0.5.0",
    "@types/better-sqlite3": "^7.6.11",
    "@types/express": "^5.0.0",
    "pino-pretty": "^13.0.0",
    "rimraf": "^6.0.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "tsx": "^4.7.2",
    "typescript": "^5.4.4"
  },
  "tsup": {
    "entry": [
      "src",
      "!src/**/__tests__/**",
      "!src/**/*.test.*"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  }
}
